{
  "index": "/index.html",
  "dataGroups": [
    {
      "name": "CryptoCompareApi",
      "urls": [
        "https://min-api.cryptocompare.com/data/**"
      ],
      "version": 1,
      "cacheConfig": {
        "maxSize": "10",
        "maxAge": "30m",
        "strategy": "performance"
      }
    }
  ],
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js"
        ]
      }
    }, {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**"
        ]
      }
    }
  ]
}
